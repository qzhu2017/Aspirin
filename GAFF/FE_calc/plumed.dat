# vim:ft=plumed

# CV computation
q6: Q6 SPECIES=13-672:21 SWITCH={CUBIC D_0=1.37 D_MAX=1.42} VMEAN MEAN

# Bias potential
opes: OPES_METAD ARG=q6.mean PACE=500 BARRIER=100 TEMP=300 

# Restriction
UPPER_WALLS ARG=q6.mean AT=0.225 KAPPA=200000 EXP=2 LABEL=uwall 
LOWER_WALLS ARG=q6.mean AT=0.100 KAPPA=200000 EXP=2 LABEL=lwall 

PRINT STRIDE=500  ARG=q6.* FILE=COLVAR
