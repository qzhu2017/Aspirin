# vim:ft=plumed

ENVIRONMENTSIMILARITY ...
 SPECIES=1-1024:21
 SIGMA=replace
 CRYSTAL_STRUCTURE=CUSTOM
 REFERENCE_1=../env1.pdb 
 REFERENCE_2=../env2.pdb
 LABEL=s
 MEAN
... ENVIRONMENTSIMILARITY

hh: HISTOGRAM ...
   DATA=s
   GRID_MIN=-0.5
   GRID_MAX=2
   GRID_BIN=1000
   BANDWIDTH=0.01
...

DUMPGRID GRID=hh FILE=histo

PRINT STRIDE=1  ARG=* FILE=COLVAR
